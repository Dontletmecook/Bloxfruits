-- Load Rayfield library
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/SiriusSoftwareLtd/Rayfield/main/source.lua'))()

-- Define your Roblox user ID
local PRIMARY_USER_ID = 1691158127 -- Replace with your actual user ID
local ALT_USER_ID = 4619471243 -- Replace with your alt user ID

-- Function to check if the current user is authorized
local function isAuthorizedUser()
    local playerId = game.Players.LocalPlayer.UserId
    return playerId == PRIMARY_USER_ID or playerId == ALT_USER_ID
end

-- Create the main window with key system
local Window = Rayfield:CreateWindow({
    Name = "Ducky - Scripts",
    LoadingTitle = "Interface Loading...",
    LoadingSubtitle = "By Ducky",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "Ducky", -- Create a custom folder for your hub/game
        FileName = "Ducky"
    },
    Discord = {
        Enabled = true,
        Invite = "4zQbh7KMNA", -- The Discord invite code
        RememberJoins = true -- Set this to false to make them join the discord every time they load it up
    },
    KeySystem = not isAuthorizedUser(), -- Enable key system only if the user is not authorized
    KeySettings = {
        Title = "Roblox Script",
        Subtitle = "Key System",
        Note = "Enter the key to proceed",
        FileName = "Ducky - Key",
        SaveKey = false,
        GrabKeyFromSite = false, -- Set this to true to get the key from a site
        Key = "Quack" -- The key to access the script
    }
})

-- Notify user
Rayfield:Notify({
    Title = "Script from Ducky",
    Content = "Thank you for using our script!",
    Duration = 6.5,
    Image = 4483362458,
    Actions = {
        Ignore = {
            Name = "Quack!",
            Callback = function()
                print("The user Just Quacked!")
            end
        }
    }
})

-- Execute the KeyTracker script
local success, keyTracker = pcall(function()
    return loadstring(game:HttpGet('https://raw.githubusercontent.com/Dontletmecook/Apple/main/KeyTracker'))()
end)

if not success then
    Rayfield:Notify({
        Title = "KeyTracker Error",
        Content = "An error occurred while loading the KeyTracker script: " .. tostring(keyTracker),
        Duration = 6,
        Image = 4483362458
    })
end

-- Create the "Main" tab
local Tab = Window:CreateTab("Main", 4483362458)

-- Function to create buttons with error handling
local function createButton(name, url)
    Tab:CreateButton({
        Name = name,
        Callback = function()
            local success, scriptCode = pcall(function()
                return game:HttpGet(url)
            end)
            if success then
                local execSuccess, execResult = pcall(function()
                    loadstring(scriptCode)()
                end)
                if not execSuccess then
                    Rayfield:Notify({
                        Title = "Script Execution Error",
                        Content = "An error occurred while executing the script: " .. execResult,
                        Duration = 6,
                        Image = 4483362458
                    })
                end
            else
                Rayfield:Notify({
                    Title = "Script Download Error",
                    Content = "An error occurred while downloading the script: " .. scriptCode,
                    Duration = 6,
                    Image = 4483362458
                })
            end
        end
    })
end

-- Add buttons to the "Main" tab
createButton("Winterhub", "https://raw.githubusercontent.com/Yatsuraa/Yuri/main/Winterhub_V2.lua")
createButton("MVSD", "https://raw.githubusercontent.com/7hbl/hitbox/main/hitbox")
createButton("Bedwars", "https://raw.githubusercontent.com/7GrandDadPGN/VapeV4ForRoblox/main/loadstring")
createButton("Brookhaven RP", "https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source")
createButton("Headless", "https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source") -- Reuse Brookhaven URL for Headless script
createButton("Blade Ball", "https://api.luarmor.net/files/v3/loaders/6af56c1753ac6679dee3acbd1fd952e5.lua")
createButton("Rivals", "https://raw.githubusercontent.com/tbao143/thaibao/main/TbaoHubRivals")
createButton("Arsenal", "https://raw.githubusercontent.com/cris123452/my/main/cas")
createButton("Eat Slime", "https://raw.githubusercontent.com/7hbl/eat-slime-to-get-huge/main/by%20%40lqmc")
createButton("Pressure", "https://raw.githubusercontent.com/ltseverydayyou/uuuuuuu/main/pressure%20god.lua")

-- Create the "SerenadeHub" tab
local Tab2 = Window:CreateTab("SerenadeHub", 4483362458)

-- Add a button to the "SerenadeHub" tab
Tab2:CreateButton({
    Name = "SerenadeHub",
    Callback = function()
        local scriptUrl = "https://raw.githubusercontent.com/4xdhondiscord/SerenadeHub/main/Serenade"
        local success, scriptCode = pcall(function()
            return game:HttpGet(scriptUrl)
        end)
        if success then
            local execSuccess, execResult = pcall(function()
                loadstring(scriptCode)()
            end)
            if not execSuccess then
                Rayfield:Notify({
                    Title = "Script Execution Error",
                    Content = "An error occurred while executing the script: " .. execResult,
                    Duration = 6,
                    Image = 4483362458
                })
            end
        else
            Rayfield:Notify({
                Title = "Script Download Error",
                Content = "An error occurred while downloading the script: " .. scriptCode,
                Duration = 6,
                Image = 4483362458
            })
        end
    end
})

-- Create the "Credits" tab
local Tab3 = Window:CreateTab("Credits", 4483362458)

-- Add a paragraph to the "Credits" tab with Discord instructions
Tab3:CreateParagraph({
    Title = "Discord Server",
    Content = "We invite you to join our Discord server: [https://discord.gg/4zQbh7KMNA](https://discord.gg/4zQbh7KMNA). Ensure you have the Discord app installed for the best experience. If you don't have it installed, you can download it from the Discord website."
})

-- Add a button to open Discord link
Tab3:CreateButton({
    Name = "Open Discord Server",
    Callback = function()
        pcall(function()
            setclipboard("https://discord.gg/4zQbh7KMNA")
            Rayfield:Notify({
                Title = "Link Copied!",
                Content = "The Discord server link has been copied to your clipboard.",
                Duration = 5,
                Image = 4483362458
            })
        end)
    end
})
